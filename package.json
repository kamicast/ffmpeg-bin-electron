{
  "name": "ffmpeg-bin-electron",
  "version": "1.0.5",
  "description": "Static ffmpeg binaries for use with electron on Mac OSX, Linux, and Windows",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "lint+build+test": "yarn run lint && yarn run build && yarn run test",
    "lint": "echo linting... && standard ./src/**/*.js && echo 'successfully linted'",
    "build": "echo building... && babel src/ -d lib/ && echo 'successfully built'",
    "build-bin": "node getBinaries.js",
    "prepublish": "yarn run build",
    "test": "echo running server tests... && standard && tape test/**/*.js | tap-summary && echo 'successfully completed tests'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kamicast/ffmpeg-bin-electron.git"
  },
  "author": "Craig OConnor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kamicast/ffmpeg-bin-electron/issues"
  },
  "homepage": "https://github.com/Kamicast/ffmpeg-bin-electron#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-flow": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "async-waterfall": "^0.1.5",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "core-js": "3",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "ffbinaries": "^1.1.1",
    "flow-bin": "^0.97.0",
    "standard": "*",
    "tape": "^4.10.1",
    "yarn-upgrade-all": "^0.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
